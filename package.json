{
  "name": "binapi",
  "version": "1.0.0",
  "description": "A biz framework base on Koa",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run doc-dev && cross-env NODE_ENV=development nodemon --watch src --watch config src/index.js",
    "start": "node src/index.js",
    "compile": "npm run doc && \\cp -rf $PWD/src/. $PWD/app && babel src/ --out-dir app/ && rm $PWD/app/index.js $PWD/app/.babelrc",
    "lint": "eslint src/**/*.js",
    "prod": "cross-env NODE_ENV=production node app/index.js",
    "doc-dev": "apidoc -c config -i src/router -o src/static/apidoc --private false && apidoc -c config -i src/router -o src/static/apidoc-internal",
    "doc": "apidoc -i src/router -o src/static/apidoc --private false && apidoc -i src/router -o src/static/apidoc-internal"
  },
  "repository": {
    "type": "git",
    "url": "git@git.ajaxp.com:helibin/sample-backend.git"
  },
  "keywords": [
    "backend"
  ],
  "author": "helibin@139.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.1",
    "bytes": "^3.0.0",
    "chalk": "^2.4.1",
    "config": "^1.30.0",
    "cross-env": "^5.2.0",
    "crypto": "^1.0.1",
    "gd-bmp": "^1.2.4",
    "ip": "^1.1.5",
    "joi": "^13.4.0",
    "js-yaml": "^3.12.0",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-cors": "0.0.16",
    "koa-nunjucks-2": "^3.0.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-useragent": "^1.1.0",
    "log4js": "^2.10.0",
    "mathjs": "^5.0.0",
    "maxmind": "^2.7.0",
    "mysql2": "^1.5.3",
    "raven": "^2.6.3",
    "redis": "^2.8.0",
    "sequelize": "^4.38.0",
    "sorted-json": "^0.2.2",
    "svg-captcha": "^1.3.11",
    "systeminformation": "^3.42.0",
    "trek-captcha": "^0.4.0",
    "uuid": "^3.3.0",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.17.5"
  }
}
