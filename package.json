{
  "name": "manage-api",
  "version": "1.0.0",
  "description": "A biz framework base on Koa",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=debug npm run doc-dev && node src/index.js nodemon",
    "dev": "cross-env NODE_ENV=dev nodemon src/index.js",
    "qa": "cross-env NODE_ENV=qa node src/index.js",
    "prod": "cross-env NODE_ENV=prod node src/index.js",
    "compile": "\\cp -rf $PWD/src/. $PWD/app && babel src/ --out-dir app/ && rm $PWD/app/index.js",
    "lint": "eslint src/**/*.js",
    "doc-dev": "apidoc -c config -i src/router -o src/static/apidoc --private false && apidoc -c config -i src/router -o src/static/mapidoc && \\cp -rf $PWD/src/static/*apidoc $PWD/app/static/",
    "doc": "apidoc -i src/router -o src/static/apidoc --private false && apidoc -i src/router -o src/static/mapidoc && \\cp -rf $PWD/src/static/*apidoc $PWD/app/static/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@git.ajaxp.com:helibin/binapi.git"
  },
  "author": "Lybeen",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/polyfill": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@sentry/node": "^5.7.1",
    "ali-mns": "^2.6.8",
    "ali-oss": "^6.1.1",
    "axios": "^0.19.0",
    "bluebird": "^3.7.1",
    "bytes": "^3.1.0",
    "captchapng": "0.0.1",
    "casbin": "^3.0.4",
    "chalk": "^2.4.2",
    "config": "^3.2.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.8.16",
    "handlebars-helpers": "^0.10.0",
