{{> header.base }}

<div class="container">
  <div class="page-header">
    <h1>发生错误<small>{{pageData.err.status}}</small></h1>
  </div>

  <p>错误代码：{{pageData.err.code}}</p>
  <p>错误代码：{{pageData.err.msg}}</p>
  {{#if pageData.err.data}}
  <p>详细内容：</p>
  <pre>{{JSONstringify pageData.err.data}}</pre>
  {{/if}}
  <p>Req Id：<code can-copy>{{pageData.err.req_id}}</code></p>
</div>

{{> footer }}
{{> footer.base }}

<script src="/js/index.js"></script>
