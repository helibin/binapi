[  {    "type": "post",    "url": "/auth/sign-in",    "title": "用户登录",    "version": "0.1.0",    "description": "<p>用户登录接口</p>",    "name": "signIn",    "group": "Auth",    "permission": [      {        "name": "admin"      }    ],    "header": {      "fields": {        "Header": [          {            "group": "Header",            "optional": true,            "field": "x-auth-token",            "description": "<p>令牌</p>"          }        ]      }    },    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "identifier",            "description": "<p>用户名.</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": false,            "field": "password",            "description": "<p>密码(md5值).</p>"          }        ]      }    },    "error": {      "fields": {        "Error 4xx": [          {            "group": "Error 4xx",            "optional": false,            "field": "noSuchUser",            "description": "<p>用户不存在</p>"          },          {            "group": "Error 4xx",            "optional": false,            "field": "userIsExisted",            "description": "<p>用户存在</p>"          },          {            "group": "Error 4xx",            "optional": false,            "field": "invildUsenameOrPassowrd",            "description": "<p>用户名或密码错误</p>"          }        ]      },      "examples": [        {          "title": "Error-Response:",          "content": "HTTP/1.1 401 Unauthorized\n{\n   \"err\": 1000,\n   \"msg\": \"noSuchUser\",\n   \"msg_locale\": \"用户不存在\",\n   \"requestId\": \"reqesutId\"\n}\n\nHTTP/1.1 401 Unauthorized\n{\n   \"err\": 1060,\n   \"msg\": \"invildUsenameOrPassowrd\",\n   \"msg_locale\": \"用户名或密码错误\",\n   \"requestId\": \"reqesutId\"\n}",          "type": "json"        }      ]    },    "sampleRequest": [      {        "url": "http://l4000.lt/v1/auth/sign-in"      }    ],    "filename": "src/router/authAPIRouter.js",    "groupTitle": "Auth",    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "err",            "description": "<p>错误代码.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "msg",            "description": "<p>错误消息.</p>"          },          {            "group": "Success 200",            "type": "Object",            "optional": true,            "field": "data",            "description": "<p>额外数据.</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n   \"err\": 0,\n   \"msg\": \"OK\",\n   \"msg_locale\": \"成功\",\n   \"data\": data,\n   \"requestId\": \"reqesutId\"\n }",          "type": "json"        }      ]    }  }]
